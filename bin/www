#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require('../app');
const path = require('path');

/**
 * Get port from environment and store in Express.
 */

var port = process.env.PORT || '3000';
app.set('port', port);
app.set('views', './views')
app.set('view engine','pug')

app.listen(3000, ()=>{
  console.log(`localhost:${port}에서 listen...`)
})